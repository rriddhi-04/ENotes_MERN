{
  "name": "enotes-mern",
  "version": "1.0.0",
  "description": "Full-stack MERN notes application",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "cd backend && npm run dev",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend/notes-app && npm install",
    "build-frontend": "cd frontend/notes-app && npm run build",
    "install-all": "npm run install-backend && npm run install-frontend",
    "heroku-postbuild": "npm run install-all && npm run build-frontend"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": ["mern", "notes", "react", "express", "mongodb"],
  "author": "",
  "license": "ISC"
}